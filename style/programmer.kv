<Programmer>:
	name: 'pro'
	
	md_bg_color: rgba(0,0,0,255)
    
    MDBoxLayout:
        orientation: 'vertical'
        padding:0,0
      
        FitImage:
            source: 'style/image/BtsBg.jpeg'
    
    FitImage:
        source: 'style/image/logo.png'
        size_hint: '.6sp','.45sp'
        pos_hint: {'center_x': .5 , 'center_y': .93}
    
    MDCard:
        id: Card
        pos_hint: {'center_x': .5, 'center_y': .55}
        size_hint: (.75, .6)
        style: 'elevated'
        
        
        
            
        FitImage:
            source: 'style/image/textfield.png'
            size_hint: 1,1
            radius: ('17sp')
			
    MDIconButton:
        icon: 'arrow-right-bold-circle-outline'
        theme_icon_color: "Custom"
        icon_color: 'black'
        style: 'tonal'
        pos_hint: {"center_x": .9, 'center_y': .96}
        on_release: 
			app.root.current= 'Menu'
			app.root.transition.direction= "left"
        